.container {
    width: 100%;
    height: 100%;
    align-items: flex-end;
    justify-content: flex-end;
}

.tooltip-container {
    width: 400px;
    height: 200px;
    background-color: rgba(79, 79, 79, 0.5);
    margin-right: 50px;
    margin-bottom: 50px;
}

.tooltip-text {
    width: 100%;
    height: 100%;
    -unity-text-align: middle-center;
    font-size: 30px;
    color: rgba(255, 255, 255, 255);
}
